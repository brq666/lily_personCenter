<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.gwhere.mapper.ResourceSetMapper" >
  	<select id="getResourceSetById" resultType="org.gwhere.model.TblPcResourceSet">
		select s.*
		from tbl_pc_resource_set s
		where id = #{id}
		and s.status = 1
	</select>
</mapper>